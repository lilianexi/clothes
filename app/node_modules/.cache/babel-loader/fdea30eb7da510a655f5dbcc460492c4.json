\"checkbox\" class=\"circle\">全选\r\n                 \r\n                        <span>合计：￥0.00</span>\r\n                        <button>结算</button>\r\n                    \r\n                </div>\r\n        <Nav></Nav>\r\n    </div>\r\n</template>\r\n<script>\r\n  import Nav from '@/components/nav.vue'\r\n  import Header from '@/components/header.vue'\r\n  import {Toast} from 'mint-ui'\r\n  export default{\r\n      data(){\r\n          return{\r\n              val:1,\r\n              list:[]\r\n          }\r\n      },\r\n      methods:{\r\n        goodAdd(){\r\n            this.val++;\r\n        },\r\n        goodSub(){\r\n            if(this.val > 1){\r\n          this.val--;\r\n        }\r\n      },\r\n     getList(){\r\n         this.axios.get(\"http://127.0.0.1:3000/cartlist?uid=2\").then(result=>{\r\n             console.log(result)\r\n                    this.list=result.data.data;\r\n                    for(var item of this.list){\r\n                        item.cb=false;\r\n                        item.cb=this.cb;\r\n                    }\r\n                })\r\n         \r\n     },\r\n     delitem(e){\r\n                //e事件对象 ，target 触发事件对象即button  dataset：自定义属性的集合。 与data-后面的值保持一致\r\n                var id=e.target.dataset.id;\r\n                var idx=e.target.dataset.idx;\r\n                console.log(idx)\r\n                var url=\"http://127.0.0.1:3000/delcartitem?id=\"+id\r\n                this.axios.get(url).then(result=>{\r\n                    if(result.data.code==1) Toast(\"删除成功\");\r\n                    this.list.splice(idx,1)   //删除数组中对应下标的商品\r\n                })\r\n            },\r\n      },\r\n       components:{\r\n           Nav,\r\n           Header\r\n       },\r\n       created(){\r\n           this.getList();\r\n       }\r\n  }\r\n</script>\r\n<style>\r\n    .mui-card.cardList{\r\n        margin:55px 0 0 0;\r\n    }\r\n    .circle{\r\n        display:inline-block;\r\n        width:27px;\r\n        height:20px;\r\n        border-radius:50%;\r\n        border:1px solid #ccc;\r\n      \r\n\r\n    }\r\n    .contList{\r\n     display:flex;\r\n     justify-content:space-between;\r\n    }\r\n     .mui-card-content-inner .circle{\r\n        margin-top:25px\r\n     }\r\n    .mui-card-content-inner .pic{\r\n        width:20%;\r\n        height:20%\r\n    }\r\n    .mui-card-content-inner .details label{\r\n        font-size:20px;\r\n        color:red;\r\n    }\r\n    .mui-card-content {border-top:1px solid #ccc}\r\n     .mui-card-content p{float:right}\r\n    /* 底部结算位置 */\r\n    .total{\r\n        width:100%;\r\n        height:44px;\r\n        background:#fff;\r\n        position: fixed;\r\n        bottom:53px;\r\n        font-size:15px\r\n    }\r\n    .total .circle{\r\n        margin:13px 15px 1px 10px;\r\n        padding: 11px;\r\n        width: 14px\r\n    }\r\n    .total  span{margin-left:142px}\r\n    .total button{\r\n        color: #fff;\r\n        background: #f50;\r\n        margin-left: .32rem;\r\n        height:46px;\r\n        width:17%\r\n    }\r\n   /*   .mui-card-content-inner .details .mui-numbox button{border:none}*/\r\n</style>"],"sourceRoot":"src\\components\\goods"}]}