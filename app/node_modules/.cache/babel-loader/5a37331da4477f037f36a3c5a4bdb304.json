r\n        search(){\r\n            var url=\"http://127.0.0.1:3000/search?pno=\"+this.pno+\"&pageSize=\"+this.pageSize+\"&key=\"+this.key;\r\n            this.axios.get(url).then(result=>{\r\n            this.key=this.keywords;\r\n            this.list=result.data.data\r\n            })\r\n        },\r\n        getpage(){\r\n                this.pno++;\r\n                var url=\"http://127.0.0.1:3000\"+ \"/products?pno=\" +this.pno+ \"&pageSize=\" +this.pageSize;\r\n                this.axios.get(url).then(result=>{\r\n                var rows=this.list.concat(result.data.data)\r\n                this.list=rows;\r\n                })\r\n            },\r\n            // 获取商品详细信息\r\n        getInfo(e){\r\n         var lid = e.target.dataset.lid;\r\n         console.log( lid)\r\n         this.$router.push(\"/goodDetail?pid=\"+lid);\r\n      },\r\n        onSwipeUp(){\r\n            // console.log(\"up\")\r\n                this.pno++;\r\n                var url=\"http://127.0.0.1:3000\"+\"/products?pno=\"+this.pno+\"&pageSize=\"+this.pageSize;\r\n                this.axios.get(url).then(result=>{\r\n                    //保留前面的内容，从concat拼接\r\n                    //console.log(result.data.data)\r\n                    var rows=this.list.concat(result.data.data)\r\n                    this.list=rows;\r\n                })\r\n            },\r\n        onSwipeDown(){\r\n            console.log(\"down\")\r\n        },\r\n        onSwipeLeft(){console.log(\"left\")},\r\n        onSwipeRight(){console.log(\"right\")}\r\n     \r\n    },\r\n    created(){\r\n        this.onSwipeUp();\r\n        this.onSwipeDown();\r\n        this.onSwipeRight();\r\n        this.onSwipeLeft();\r\n        this.getpage();\r\n        // this.search()\r\n    },\r\n      components:{\r\n           Nav\r\n       }\r\n       \r\n\r\n}\r\n</script>\r\n<style>\r\n.mui-bar{\r\n    display: flex;\r\n    justify-content: space-between;\r\n}\r\n.mui-bar .mui-input-row .mui-input-clear{\r\n    border:1px solid #fff;\r\n    margin-left:0\r\n}\r\n.mui-bar .mui-action-back,.mui-icon-search{\r\n    color:#fff\r\n}\r\n.mui-bar input[type='search']::-webkit-input-placeholder {\r\n    color:#ccc\r\n}\r\n\r\n.app-list .content{\r\n        list-style: none;\r\n        display: flex;\r\n        flex-wrap:wrap;\r\n        justify-content: space-between;\r\n        padding-left:0;\r\n        margin-top:44px\r\n        }\r\n    .app-list  .content .content-item{\r\n        width:50%;\r\n        background:#fff;\r\n        border:1px solid #ccc;\r\n        border-radius: 10px;\r\n        margin-top:10px\r\n    }\r\n    .app-list  .content .content-item img{\r\n        width:100%;\r\n      \r\n    }\r\n    .app-list  .wz{\r\n        text-align: center;\r\n        font-size: 12px;\r\n        color:#c2beb2;\r\n        font-weight: bold;\r\n        overflow: hidden;\r\n    }\r\n    .app-list  .price{\r\n        display: inline-block;\r\n        color:red;\r\n        font-size:24px;\r\n        margin-top:11px\r\n    }\r\n</style>\r\n\r\n\r\n       "],"sourceRoot":"src\\components\\goods"}]}