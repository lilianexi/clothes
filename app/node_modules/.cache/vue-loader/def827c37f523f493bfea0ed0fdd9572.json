;AACA","file":"goodList.vue","sourceRoot":"src\\components\\goods","sourcesContent":["<template>\r\n<!--父元素-->\r\n    <div class=\"app-goodlist\">\r\n<!--商品-->\r\n        <div class=\"good-item\" v-for=\"item in list\" :key=\"item.fid\">\r\n            <img :src=\"'http://127.0.0.1:3000/'+item.md\" @click=\"jumpInfo\" :data-fid=\"item.fid\"/>\r\n            <h4>{{item.fname}}</h4>\r\n            <div class=\"info\">\r\n                <span class=\"now\">{{item.price}}</span>\r\n            </div>\r\n            <div class=\"sell\">\r\n                <span>热卖中</span>\r\n            </div>\r\n        </div>\r\n         <mt-button type=\"primary\" size=\"large\"@click=\"getMore\">加载更多</mt-button>\r\n    </div>\r\n</template>\r\n<script>\r\n    export default{\r\n        data(){\r\n            return{\r\n                list:[],\r\n                pno:0,\r\n                pageSize:4\r\n            }\r\n        },\r\n        created(){\r\n            this.getpage();\r\n        },\r\n        methods:{\r\n\t    jumpInfo(e){\r\n         var fid = e.target.dataset.fid;\r\n         console.log(fid)\r\n         //跳转新闻详情组件\r\n         this.$router.push(\"/GoodInfo?pid=\"+fid);\r\n      },\r\n            getpage(){\r\n                this.pno++;\r\n                var url=\"http://127.0.0.1:3000\"+ \"/products?pno=\" +this.pno+ \"&pageSize=\" +this.pageSize;\r\n             // var url=\"http://127.0.0.1:3000\";\r\n              //url+=\"/products?pno=\"+this.pno;\r\n             // url+=\"&pageSize=\"+this.pageSize;\r\n                this.axios.get(url).then(result=>{\r\n                    //console.log(result.data.data)\r\n                   var rows=this.list.concat(result.data.data)\r\n                   this.list=rows;\r\n                })\r\n            },\r\n               getMore(){\r\n                //加载下一页的数据\r\n                //console.log(123)\r\n                //修改当前页码+1 发送请求get，获取下一页数据，保存到list\r\n                this.pno++;\r\n                var url=\"http://127.0.0.1:3000\"+\"/products?pno=\"+this.pno+\"&pageSize=\"+this.pageSize;\r\n                this.axios.get(url).then(result=>{\r\n                    //保留前面的内容，从concat拼接\r\n                    //console.log(result.data.data)\r\n                    var rows=this.list.concat(result.data.data)\r\n                    this.list=rows;\r\n                })\r\n            }\r\n        },\r\n        \r\n    }\r\n</script>\r\n<style>\r\n    .app-goodlist{\r\n        display:flex;\r\n        flex-wrap:wrap;\r\n        justify-content:space-between;\r\n        padding:4px;\r\n    }\r\n    .app-goodlist .good-item{\r\n        width:49%;\r\n        border:1px solid #ccc;\r\n        box-shadow:0 0 8px #ccc;\r\n        margin:4px 0;\r\n        padding:2px;\r\n        display:flex;\r\n        flex-direction:column;\r\n        min-height:230px;\r\n        justify-content:space-between;\r\n    }\r\n    .app-goodlist .good-item img{\r\n        width:100%;\r\n    }\r\n     .app-goodlist .good-item h4{\r\n        font-size:18px;\r\n    }\r\n     .app-goodlist .good-item .now{\r\n        color:red;\r\n        font-size:16px;\r\n        font-weight:bold;\r\n    }\r\n</style>"]}]}